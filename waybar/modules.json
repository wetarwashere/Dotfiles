{
    "group/power": {
        "orientation": "horizontal",
        "drawer": {
            "transition-duration": 800,
            "click-to-reveal": true,
            "transition-left-to-right": true,
        },
        "modules": [
            "custom/power",
            "custom/reboot",
            "custom/off",
        ],
    },
    "tray": {
        "icon-size": 23,
        "spacing": 10,
    },
    "custom/arch": {
        "format": "",
        "tooltip": false,
        "on-click": "$HOME/.config/waybar/scripts/swifty",
        "on-click-right": "$HOME/.config/waybar/scripts/wrandom",
    },
    "custom/power": {
        "format": "󱄅",
        "tooltip": false,
    },
    "custom/off": {
        "format": "",
        "tooltip": false,
        "on-click": "poweroff",
    },
    "custom/reboot": {
        "format": "",
        "tooltip": false,
        "on-click": "reboot",
    },
    "wireplumber": {
        "format": "{icon} {volume}%",
        "format-muted": "󰝟 Muted",
        "on-click": "pactl set-sink-mute alsa_output.pci-0000_03_00.6.analog-stereo toggle && $HOME/.config/waybar/scripts/volumestate",
        "tooltip": false,
        "format-icons": ["󰕿", "󰖀", "󰕾"],
        "on-scroll-up": "pactl set-sink-volume alsa_output.pci-0000_03_00.6.analog-stereo +2% && $HOME/.config/waybar/scripts/devvolume",
        "on-scroll-down": "pactl set-sink-volume alsa_output.pci-0000_03_00.6.analog-stereo -2% && $HOME/.config/waybar/scripts/devvolume"
    },
    "mpd": {
        "format": "{stateIcon} {title}",
        "format-paused": "{stateIcon} {title}",
        "state-icons": {
            "paused": "",
            "playing": " ",
        },
        "format-stopped": "",
        "on-click": "mpc toggle && $HOME/.config/waybar/scripts/songinfo",
        "on-scroll-up": "mpc volume +2 && $HOME/.config/waybar/scripts/songvolume",
        "on-scroll-down": "mpc volume -2 && $HOME/.config/waybar/scripts/songvolume",
        "tooltip": false,
    },
    "battery": {
        "interval": 10,
        "bat": "BAT1",
        "states": {
            "warning": 30,
            "critical": 15
        },
        "format": "{icon} {capacity}%",
        "format-charging": "󰂄 {capacity}%",
        "format-discharging": "󱟨 {capacity}%",
        "format-icons": [ "󰁺", "󰁻", "󰁼", "󰁽", "󰁾", "󰁿", "󰂀", "󰁹" ],
        "max-length": 25,
        "tooltip": false
    },
    "hyprland/workspaces": {
        "disable-scroll": true,
        "all-outputs": true,
        "format":"{icon}",
        "format-icons": {
            "default": "󰝤",
        },
        "persistent-workspaces": {
            "*": 4,
        },
    },
    "cava": {
        "bars": 9,
        "format-icons": ["▁", "▂", "▃", "▄", "▅", "▆", "▇", "█" ],
        "waves": true,
        "monstercat": true,
        "bar_delimiter": 32,
        "method": "pipewire",
        "input_delay": 0,
        "lower_cutoff_freq": 5,
        "higher_cutoff_freq": 10000,
    },
    "clock": {
        "format": "󰥔 {:%I:%M %p}",
        "format-alt": "󰥔 {:%b %d %a %Y}",
        "interval": 60,
        "timezone": "Asia/Jakarta",
        "tooltip": false,
    },
    "network": {
        "interval": 10,
        "format-icons": [
            "󰤯",
            "󰤟",
            "󰤢",
            "󰤥",
            "󰤨"
        ],
        "format": "{icon} {signalStrength}%",
        "format-disconnected": "󰤮 Unknown",
        "tooltip": false,
    },
}
